func ip_to_code(ip:String):
	var x = ""
	for i in ip.split(ip_delimiter): # 
		x += words[int(i)] + code_delimiter
	return x.left(-1)

func code_to_ip(code:String):
	var x = ""
	for i in code.split(code_delimiter):
		x += str(words.find(i)) + ip_delimiter
	return x.left(-1)
